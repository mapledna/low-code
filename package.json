{
  "name": "form-generator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode dev",
    "dev": "vue-cli-service serve --mode dev",
    "copy:theme": "node build/copy-theme.js",
    "build-dev": "vue-cli-service build --mode build-dev && npm run copy:theme",
    "build": "vue-cli-service build --mode build-prod && npm run copy:theme",
    "build-dev:designer": "cross-env PROJECT_TYPE=designer npm run build-dev",
    "build-dev:project": "cross-env PROJECT_TYPE=project npm run build-dev",
    "build:designer": "cross-env PROJECT_TYPE=designer npm run build",
    "build:project": "cross-env PROJECT_TYPE=project npm run build",
    "lint": "vue-cli-service lint"
  },
